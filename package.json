{
	"name": "insight",
	"version": "1.0.0",
	"type": "module",
	"bin": {
		"insight": "./cli/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "node -e \"import('./dist/cli/index.js').then(m => m.default)\" init",
		"web:dev": "cd web && pnpm dev",
		"web:build": "npm run build && cd web && pnpm build"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^1.2.12",
		"@ai-sdk/google": "^1.2.22",
		"@ai-sdk/openai": "^1.3.22",
		"@ai-sdk/openai-compatible": "^0.2.14",
		"@libsql/client": "^0.15.9",
		"@tailwindcss/typography": "^0.5.16",
		"ai": "^4.3.16",
		"commander": "^14.0.0",
		"drizzle-orm": "^0.44.2",
		"react-markdown": "^10.1.0",
		"zod": "^3.25.74"
	},
	"devDependencies": {
		"@types/node": "^24.0.10",
		"drizzle-kit": "^0.31.4",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3"
	}
}
